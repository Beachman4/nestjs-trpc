{
  "name": "nestjs-trpc",
  "version": "1.0.0",
  "homepage": "https://nestjs-trpc.io",
  "main": "./dist/index.js",
  "types": "./src/index.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./types": {
      "import": "./dist/types/index.js",
      "require": "./dist/types/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "scripts": {
    "lint": "tslint -p tsconfig.json -c tslint.json --fix",
    "build": "tsc -b -v",
    "start:dev": "tsc -w",
    "debug:dev": "ts-node --inspect-brk lib/index.ts",
    "format": "prettier \"src/**/*.ts\" --ignore-path ./.prettierignore --write",
    "clean": "tsc -b --clean",
    "test": "export TS_JEST_DISABLE_VER_CHECKER=true && jest --no-watchman --coverage",
    "prepare": "husky install & pnpm run build"
  },
  "keywords": [],
  "author": {
    "name": "Kevin Edry",
    "email": "kevin.edry@gmail.com",
    "url": "https://kevin-edry.com"
  },
  "license": "MIT",
  "engines": {
    "node": "14 || 15 || 16 || 17 || 18 || 19 || 20",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.12.1",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@trpc/server": "^10.18.0",
    "reflect-metadata": "^0.1.13",
    "ts-morph": ">= 22.0.0",
    "zod": ">= 3.14.3"
  },
  "peerDevDependencies": {
    "@types/node": "20.14.10"
  },
  "peerDependenciesMeta": {
    "ts-morph": {
      "optional": true
    },
    "zod": {
      "optional": true
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^17.5.1",
    "@commitlint/config-angular": "^17.4.4",
    "@eslint/js": "^9.6.0",
    "@nestjs/cli": "^9.3.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "29.2.4",
    "@types/lodash": "^4.17.5",
    "eslint": "^8.37.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.0",
    "release-it": "^15.10.0",
    "rimraf": "^4.4.1",
    "ts-jest": "29.0.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.5.0",
    "type-fest": "^4.21.0",
    "typescript": "^5.0.3"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "func-loc": "^0.1.16",
    "lodash": "^4.17.21"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/KevinEdry/nestjs-trpc"
  }
}
