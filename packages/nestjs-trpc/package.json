{
  "name": "nestjs-trpc",
  "version": "1.1.1",
  "homepage": "https://nestjs-trpc.io",
  "main": "./dist/index.js",
  "types": "./src/index.ts",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./types": {
      "import": "./dist/types/index.js",
      "require": "./dist/types/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "scripts": {
    "lint": "tslint -p tsconfig.json -c tslint.json --fix",
    "build": "tsc -b -v",
    "start:dev": "tsc -w",
    "debug:dev": "ts-node --inspect-brk lib/index.ts",
    "format": "prettier \"src/**/*.ts\" --ignore-path ./.prettierignore --write",
    "clean": "tsc -b --clean",
    "test": "export TS_JEST_DISABLE_VER_CHECKER=true && jest --no-watchman --coverage"
  },
  "keywords": [
    "nestjs",
    "trpc",
    "adapter",
    "nest",
    "express"
  ],
  "author": {
    "name": "Kevin Edry",
    "email": "kevin.edry@gmail.com",
    "url": "https://kevin-edry.com"
  },
  "packageManager": "pnpm@8.12.1",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "zod": "^3.14.0",
    "@trpc/server": "^10.18.0",
    "reflect-metadata": "^0.1.13 || ^0.2.0",
    "ts-morph": "^16.0.0 || ^17.0.0 || ^18.0.0 || ^19.0.0 || ^20.0.0 || ^21.0.0 || ^22.0.0"
  },
  "devDependencies": {
    "@nestjs/common": "9.0.0",
    "@nestjs/core": "9.0.0",
    "@nestjs/cli": "^9.3.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "29.2.4",
    "@types/lodash": "^4.17.5",
    "tsconfig-paths": "^4.2.0",
    "type-fest": "^4.21.0",
    "typescript": "^5.0.3",
    "reflect-metadata": "0.2.2",
    "ts-morph": "22.0.0"
  },
  "dependencies": {
    "func-loc": "^0.1.16",
    "lodash": "^4.17.21",
    "tslib": "^2.5.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/KevinEdry/nestjs-trpc"
  }
}
